(
    (main (x)
        (0  (ld r1 x)
            (br r1 1 2) )
        (1  (lc r2 1)
            (add r1 r1 r2)
            (br r1 3 3) )
        (2  (ld r2 x)
            (lc r3 2)
            (add r1 r1 r3)
            (br r1 3 4) )
        (3  (ld r5 x)
            (ret r5) )
        (4  (ld r6 x)
            (add r6 r6 r5)
            (ret r5) )
    )
)
 
